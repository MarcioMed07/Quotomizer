{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "devs": "nuxt dev --host --https --ssl-cert cert.pem --ssl-key cert-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prettier": "prettier --write ./**/*.ts ./**/*.vue",
    "vercel-pull": "vercel pull --yes --environment=production",
    "vercel-build": "vercel build --prod",
    "vercel-deploy": "vercel deploy --prebuilt --prod",
    "deploy": "yarn vercel-pull;yarn vercel-build;yarn vercel-deploy"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/google-fonts": "^3.1.0",
    "nuxt": "^3.8.2",
    "prettier": "^3.1.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@nuxt/ui": "^2.11.0"
  }
}
